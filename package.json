{
  "name": "@hebcal/rest-api",
  "version": "4.4.2",
  "author": "Michael J. Radwin (https://github.com/mjradwin)",
  "keywords": [
    "hebcal"
  ],
  "description": "Jewish holidays and Hebrew calendar as plain JSON objects and CSV export",
  "engines": {
    "node": ">= 16.0.0"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "typings": "rest-api.d.ts",
  "files": [
    "dist",
    "rest-api.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hebcal/hebcal-rest-api.git"
  },
  "bugs": {
    "url": "https://github.com/hebcal/hebcal-rest-api/issues"
  },
  "dependencies": {
    "@hebcal/core": "^3.50.3",
    "@hebcal/leyning": "^7.2.1",
    "@hebcal/triennial": "^1.1.0"
  },
  "scripts": {
    "build": "rollup -c",
    "prepublish": "npm run build",
    "pretest": "npm run build",
    "readme": "npx jsdoc2md dist/index.js",
    "test": "ava"
  },
  "license": "BSD-2-Clause",
  "ava": {
    "require": ["@babel/register"]
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/register": "^7.21.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "ava": "^5.2.0",
    "core-js": "^3.29.0",
    "eslint": "^8.35.0",
    "eslint-config-google": "^0.14.0",
    "jsdoc": "^4.0.2",
    "jsdoc-to-markdown": "^8.0.0",
    "rollup": "^3.17.3"
  }
}
