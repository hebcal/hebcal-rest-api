{
  "name": "@hebcal/rest-api",
  "version": "3.12.0",
  "author": "Michael J. Radwin (https://github.com/mjradwin)",
  "keywords": [
    "hebcal"
  ],
  "description": "Jewish holidays and Hebrew calendar as plain JSON objects and CSV export",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "rest-api.d.ts",
  "files": [
    "dist",
    "rest-api.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hebcal/hebcal-rest-api.git"
  },
  "bugs": {
    "url": "https://github.com/hebcal/hebcal-rest-api/issues"
  },
  "dependencies": {
    "@hebcal/core": "^3.33.2",
    "@hebcal/leyning": "^4.12.1"
  },
  "scripts": {
    "build": "rollup -c",
    "prepublish": "npm run build",
    "pretest": "npm run build",
    "readme": "npx jsdoc2md dist/index.js",
    "test": "ava"
  },
  "license": "BSD-2-Clause",
  "ava": {
    "files": [
      "src/**/*.spec.js"
    ],
    "require": [
      "@babel/register"
    ],
    "babel": {
      "testOptions": {
        "presets": [
          "@babel/env"
        ]
      }
    },
    "inherit": true,
    "verbose": true
  },
  "devDependencies": {
    "@ava/babel": "^2.0.0",
    "@babel/core": "^7.17.3",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.17.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "ava": "^3.15.0",
    "core-js": "^3.21.0",
    "eslint": "^8.9.0",
    "eslint-config-google": "^0.14.0",
    "jsdoc": "^3.6.10",
    "jsdoc-to-markdown": "^7.1.1",
    "rollup": "^2.67.2",
    "rollup-plugin-terser": "^7.0.2"
  }
}
