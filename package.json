{
  "name": "@hebcal/rest-api",
  "version": "2.3.3",
  "author": "Michael J. Radwin (https://github.com/mjradwin)",
  "keywords": [
    "hebcal"
  ],
  "description": "Jewish holidays and Hebrew calendar as plain JSON objects and CSV export",
  "main": "dist/index.js",
  "typings": "rest-api.d.ts",
  "files": [
    "dist",
    "rest-api.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hebcal/hebcal-rest-api.git"
  },
  "bugs": {
    "url": "https://github.com/hebcal/hebcal-rest-api/issues"
  },
  "dependencies": {
    "@hebcal/core": "^2.2.4",
    "@hebcal/geo-sqlite": "^2.2.5",
    "@hebcal/leyning": "^2.2.0"
  },
  "scripts": {
    "build": "rollup -c",
    "prepublish": "npm run build",
    "pretest": "npm run build",
    "readme": "npx jsdoc2md dist/index.js",
    "test": "ava"
  },
  "license": "BSD-2-Clause",
  "ava": {
    "files": [
      "src/**/*.spec.js"
    ],
    "require": [
      "@babel/register"
    ],
    "babel": {
      "testOptions": {
        "presets": [
          "@babel/env"
        ]
      }
    },
    "inherit": true,
    "verbose": true
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@babel/register": "^7.10.5",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "ava": "^3.11.1",
    "core-js": "^3.6.5",
    "eslint": "^7.6.0",
    "eslint-config-google": "^0.14.0",
    "jsdoc": "^3.6.5",
    "jsdoc-to-markdown": "^5.0.3",
    "rollup": "^2.23.1",
    "rollup-plugin-terser": "^7.0.0"
  }
}
