{
  "name": "@hebcal/rest-api",
  "version": "4.5.0",
  "author": "Michael J. Radwin (https://github.com/mjradwin)",
  "keywords": [
    "hebcal"
  ],
  "description": "Jewish holidays and Hebrew calendar as plain JSON objects and CSV export",
  "engines": {
    "node": ">= 16.0.0"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "typings": "rest-api.d.ts",
  "files": [
    "dist",
    "rest-api.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hebcal/hebcal-rest-api.git"
  },
  "bugs": {
    "url": "https://github.com/hebcal/hebcal-rest-api/issues"
  },
  "dependencies": {
    "@hebcal/core": "^4.0.1",
    "@hebcal/leyning": "^7.3.1",
    "@hebcal/triennial": "^1.2.0"
  },
  "scripts": {
    "build": "rollup -c",
    "prepublish": "npm run build",
    "pretest": "npm run build",
    "readme": "npx jsdoc2md dist/index.js",
    "test": "ava"
  },
  "license": "BSD-2-Clause",
  "ava": {
    "require": [
      "@babel/register"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/register": "^7.21.0",
    "@hebcal/learning": "^1.3.1",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-terser": "^0.4.1",
    "ava": "^5.2.0",
    "core-js": "^3.30.2",
    "eslint": "^8.40.0",
    "eslint-config-google": "^0.14.0",
    "jsdoc": "^4.0.2",
    "jsdoc-to-markdown": "^8.0.0",
    "rollup": "^3.21.7"
  }
}
